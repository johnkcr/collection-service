{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--require=dotenv/config"
            ],
            "program": "${workspaceFolder}/dist/bootstrap.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}